{
  "exclude": ["dist", "public", "data"],
  "compilerOptions": {
    "pretty": true,
    "target": "esnext",
    "module": "esnext",
    "lib": ["ES2015", "DOM", "ES2018.AsyncIterable", "ESNext"],
    "jsx": "preserve",
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": false,
    "downlevelIteration": true,
    "forceConsistentCasingInFileNames": true
  }
}
